var phrases = [
    'Ciao Mondo',
    'Hello World',
    'Hello World!',
    'hello world',
    'hello world!'],
    result,
    regex = %%%REPLACED%%%

phrases.forEach(function (phrase) {
    result = phrase.match(regex)
    if (result) {
        console.log(result)
    } else {
        console.log()
    }
})
