var worlds = ['acceso', 'appeso', 'arreso', 'riacceso', 'sovrappeso', 'acreso', 'appreso'],
    regexes = [%%%REPLACED%%%]

regexes.forEach(function (regex) {
    worlds.forEach(function (world) {
        var result = world.match(regex)
        if (result) {
            console.log(result[0])
        } else {
            console.log()
        }
    })
})
